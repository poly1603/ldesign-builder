  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "less": "^4.2.0"
  }
