{
  "name": "@examples/tdesign-like",
  "version": "1.0.0",
  "description": "类 TDesign 风格的 Vue3 组件库示例 - 使用 TSX 格式",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.cjs",
      "default": "./esm/index.js"
    },
    "./button": {
      "types": "./esm/button/index.d.ts",
      "import": "./esm/button/index.js",
      "require": "./lib/button/index.cjs",
      "default": "./esm/button/index.js"
    },
    "./button/*": {
      "types": "./esm/button/*.d.ts",
      "import": "./esm/button/*.js",
      "require": "./lib/button/*.cjs",
      "default": "./esm/button/*.js"
    },
    "./hooks/*": {
      "types": "./esm/hooks/*.d.ts",
      "import": "./esm/hooks/*.js",
      "require": "./lib/hooks/*.cjs",
      "default": "./esm/hooks/*.js"
    },
    "./icon": {
      "types": "./esm/icon/index.d.ts",
      "import": "./esm/icon/index.js",
      "require": "./lib/icon/index.cjs",
      "default": "./esm/icon/index.js"
    },
    "./icon/*": {
      "types": "./esm/icon/*.d.ts",
      "import": "./esm/icon/*.js",
      "require": "./lib/icon/*.cjs",
      "default": "./esm/icon/*.js"
    },
    "./input": {
      "types": "./esm/input/index.d.ts",
      "import": "./esm/input/index.js",
      "require": "./lib/input/index.cjs",
      "default": "./esm/input/index.js"
    },
    "./input/*": {
      "types": "./esm/input/*.d.ts",
      "import": "./esm/input/*.js",
      "require": "./lib/input/*.cjs",
      "default": "./esm/input/*.js"
    },
    "./message": {
      "types": "./esm/message/index.d.ts",
      "import": "./esm/message/index.js",
      "require": "./lib/message/index.cjs",
      "default": "./esm/message/index.js"
    },
    "./message/*": {
      "types": "./esm/message/*.d.ts",
      "import": "./esm/message/*.js",
      "require": "./lib/message/*.cjs",
      "default": "./esm/message/*.js"
    },
    "./utils/*": {
      "types": "./esm/utils/*.d.ts",
      "import": "./esm/utils/*.js",
      "require": "./lib/utils/*.cjs",
      "default": "./esm/utils/*.js"
    },
    "./esm/button/index.css": "./esm/button/index.css",
    "./esm/icon/index.css": "./esm/icon/index.css",
    "./esm/index.css": "./esm/index.css",
    "./esm/input/index.css": "./esm/input/index.css",
    "./esm/message/index.css": "./esm/message/index.css",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "esm"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:style": "ldesign-builder build --style-only"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "vue": "^3.4.0",
    "less": "^4.2.0"
  },
  "typings": "./esm/index.d.ts",
  "browser": "./dist/index.js"
}