{
  "name": "@ldesign/builder",
  "version": "1.0.0",
  "description": "智能前端库打包工具 - 基于Rollup JavaScript API的零配置多格式输出解决方案，支持TypeScript、Vue、React等多种库类型，具备完善的性能监控和代码质量分析功能",
  "keywords": [
    "rollup",
    "rolldown",
    "bundler",
    "typescript",
    "vue",
    "react",
    "library",
    "build-tool",
    "frontend",
    "smart-config",
    "auto-detect",
    "performance",
    "optimization",
    "code-quality",
    "memory-management",
    "error-handling"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/packages/builder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/packages/builder"
  },
  "bugs": {
    "url": "https://github.com/ldesign/packages/builder/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    },
    "./cli/*": {
      "types": "./dist/cli/*.d.ts",
      "import": "./dist/cli/*.js",
      "require": "./dist/cli/*.cjs"
    },
    "./config/*": {
      "types": "./dist/config/*.d.ts",
      "import": "./dist/config/*.js",
      "require": "./dist/config/*.cjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.ts",
      "import": "./dist/constants/*.js",
      "require": "./dist/constants/*.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.js",
      "require": "./dist/plugins/*.cjs"
    },
    "./tests/*": {
      "types": "./dist/tests/*.d.ts",
      "import": "./dist/tests/*.js",
      "require": "./dist/tests/*.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs"
    },
    "./__tests__/*": {
      "types": "./dist/__tests__/*.d.ts",
      "import": "./dist/__tests__/*.js",
      "require": "./dist/__tests__/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:build": "npm run build && npm run test:run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint src/core src/utils src/index.ts",
    "lint:fix": "eslint src/core src/utils src/index.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist esm cjs lib types",
    "verify:build": "node -e \"console.log('Verifying build outputs...'); const fs = require('fs'); const files = ['dist/index.js', 'dist/index.cjs', 'dist/index.d.ts', 'dist/index.d.cts', 'dist/cli/index.js', 'dist/cli/index.cjs', 'dist/cli/index.d.ts', 'dist/cli/index.d.cts']; files.forEach(f => { if (fs.existsSync(f)) console.log('✓', f); else console.error('✗', f); });\""
  },
  "dependencies": {
    "@ldesign/kit": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-preset-solid": "^1.8.0",
    "rollup-plugin-svelte": "^7.2.0",
    "svelte": "^4.2.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitejs/plugin-vue2": "^2.3.0",
    "@vitejs/plugin-vue2-jsx": "^1.0.0",
    "@vue/compiler-sfc": "^3.4.0",
    "acorn-jsx": "^5.3.2",
    "acorn-typescript": "^1.4.7",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.6.0",
    "clean-css": "^5.3.0",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "gzip-size": "^7.0.0",
    "jiti": "^1.21.0",
    "less": "^4.4.1",
    "ora": "^5.4.1",
    "postcss": "^8.5.6",
    "pretty-bytes": "^6.1.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.2.16",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-stylus": "^1.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unplugin-vue": "^7.0.1",
    "unplugin-vue-jsx": "^0.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.6.1",
    "chokidar": "^3.6.0",
    "cssnano": "^7.1.1",
    "eslint": "^8.56.0",
    "rolldown": "1.0.0-beta.35",
    "rollup": "^4.46.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "ts-node": "^10.9.0",
    "tsup": "^8.0.0",
    "vitepress": "^1.6.4",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "ldesign-builder": "./bin/ldesign-builder.js"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}